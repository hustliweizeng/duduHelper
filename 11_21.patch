-        if (upperBound < lowerBound) {  
-            return lowerBound;  
-        }  
-  
-        if ((maxNumOfPixels == -1) && (minSideLength == -1)) {  
-            return 1;  
-        } else if (minSideLength == -1) {  
-            return lowerBound;  
-        } else {  
